<div class="container">
  <div class="row">
      <%= form_for @admin do |f| %>
        <div class="field">
          <%= f.label :name %>
          <%= f.text_field :name %>
        </div>
        <div class="field">
          <%= f.label :password %>
          <%= f.password_field :password %>
        </div>
        <%= f.submit %>
      <% end %>
  </div>
</div>
